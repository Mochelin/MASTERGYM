@{
	ViewBag.Title = "Contacto";
	Layout = "~/Views/Shared/_LayoutLanding.cshtml";
}
@section Style {


	<link rel="stylesheet" href="~/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/font-awesome.min.css" />
	<link rel="stylesheet" href="~/css/owl.carousel.min.css" />
	<link rel="stylesheet" href="~/css/flaticon.css" />
	<link rel="stylesheet" href="~/css/slicknav.min.css" />


	<link rel="stylesheet" href="~/css/style2.css" />
}
<body>



	<!-- Sección superior de la página -->
	<section class="page-top-section set-bg" data-setbg="~/img/header-bg/4.jpg">
		<div class="container">
			<h2>Contacto</h2>
		</div>
	</section>
	<!-- Fin de la sección superior de la página -->
	<!-- Sección de contacto -->
	<section class="contact-section" style="background-color: white">
		<div class="container">
			<div class="row">
				<div class="col-lg-4">
					<h2 class="contact-title">Información de Contacto</h2>
					<div class="contact-info-warp">
						<h4>Ubicación</h4>
						<div class="contact-info">
							<img src="img/icons/1-dark.png" alt="">
							<div class="cf-text">
								<p>Batallón Atacama 80, Caldera, Atacama</p>
							</div>
						</div>
					</div>
					<div class="contact-info-warp">
						<h4>Suscripciones</h4>
						<div class="contact-info">
							<img src="img/icons/2-dark.png" alt="">
							<div class="cf-text">
								<p>+56 9 9986 7321</p>
							</div>
						</div>
					</div>
					<div class="contact-info-warp">
						<h4>Correo Electrónico</h4>
						<div class="contact-info">
							<img src="img/icons/3-dark.png" alt="">
							<div class="cf-text">
								<p>Contacto@mastersgym.com</p>
								<p>www.MastersGYM.com</p>
							</div>
						</div>
					</div>
				</div>
				<div class="col-lg-8">
					<h2 class="contact-title">Ponte en contacto</h2>
					<form class="contact-form">
						<div class="row">
							<div class="col-md-6">
								<input type="text" placeholder="Tu nombre">
							</div>
							<div class="col-md-6">
								<input type="text" placeholder="Tu correo electrónico">
							</div>
							<div class="col-md-12">
								<input type="text" placeholder="Asunto">
								<textarea placeholder="Mensaje"></textarea>
								<button class="site-btn">Enviar Mensaje</button>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
		<div class="map">
			<iframe src="https://www.google.com/maps/embed?pb=!1m23!1m12!1m3!1d5024.348560421256!2d-70.81995689230938!3d-27.069608521188588!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!4m8!3e6!4m0!4m5!1s0x96bd69a0a961ca01%3A0xfd522c35730cf66e!2sBatall%C3%B3n%20Atacama%2080%2C%201571185%20Caldera%2C%20Atacama!3m2!1d-27.0686592!2d-70.8174421!5e0!3m2!1ses-419!2scl!4v1734323336837!5m2!1ses-419!2scl"></iframe>
		</div>
	</section>
	<!-- Fin de la sección de contacto -->
	<!--====== Javascripts & Jquery ======-->
	<script src="~/js/jquery-3.2.1.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
	<script src="~/js/jquery.slicknav.js"></script>
	<script src="~/js/owl.carousel.min.js"></script>
	<script src="~/js/circle-progress.min.js"></script>
	<script src="~/views/main.js"></script>
</body>
